{
  "name": "hichain_web_apps",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:client": "VITE_APP_VERSION=\"development\" vite --config src/vite.config.ts",
    "start:master": "ts-node -r dotenv/config --project src/tsconfig.server.json src/server/index.ts dotenv_config_path=.env.development",
    "build": "npm-run-all -p build:*",
    "build:client": "VITE_APP_VERSION=\"`yarn run --silent app-version`\" vite --config src/vite.config.ts build",
    "build:master": "tsc --project src/tsconfig.server.json",
    "serve:client": "VITE_APP_VERSION=\"`yarn run --silent app-version`\" vite --config src/vite.config.ts preview",
    "serve:master": "node -r dotenv/config dist/server/server/index.js dotenv_config_path=.env.production",
    "typecheck": "npm-run-all -p typecheck:*",
    "typecheck:client": "tsc --project src/tsconfig.json --noEmit",
    "typecheck:master": "tsc --project src/tsconfig.server.json --noEmit",
    "tsc-watch:client": "tsc --project src/tsconfig.json --watch",
    "tsc-watch:master": "tsc --project src/tsconfig.server.json --watch",
    "lint": "npm-run-all -p lint:*",
    "lint:ts": "eslint --max-warnings=0 './src/**/*.{ts,tsx}'",
    "lint:css-in-js": "stylelint --max-warnings=0 --syntax 'css-in-js' './src/**/*.tsx'",
    "test": "jest --passWithNoTests",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "app-version": "echo \"v$npm_package_version+`git rev-parse --short HEAD`\""
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@mui/styled-engine-sc": "^5.0.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.13",
    "boardgame.io": "0.47.9",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "node-sass": "^5.0.0",
    "notistack": "^1.0.10",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-indiana-drag-scroll": "^2.0.1",
    "react-redux": "^7.2.4",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.1",
    "typed-redux-saga": "^1.3.1",
    "typescript": "~4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@jambit/eslint-plugin-typed-redux-saga": "^0.3.1",
    "@types/jest": "^27.0.1",
    "@types/koa-router": "^7.4.2",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-react": "^1.0.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux-localstorage-simple": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-jest": "^26.5.2",
    "ts-node": "^10.2.1",
    "vite": "^2.5.10",
    "vite-plugin-eslint": "^1.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/src/tsconfig.test.json"
      }
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix --max-warnings=0",
    "*.{css,scss,tsx}": "stylelint --fix --syntax 'css-in-js'"
  }
}
